<?xml version="1.0" encoding="UTF-8"?>

<loggedFS logEnabled="true" printProcessName="true">
	<includes>
		<include extension=".*" uid="*" action=".*" retname=".*"/>
	</includes>
	<excludes>
		<exclude extension=".*\.xml$" uid="*" action=".*" retname=".*"/>
		<exclude extension=".*" uid="500" action=".*" retname="FAILURE"/>
		<exclude extension=".*" uid="*" action="getattr" retname="SUCCESS"/>
	</excludes>
</loggedFS>
